[ADI][FAK](.*?)[AMM][PART]>(.*?)[(ABS|ERG|SOZ|DES|GEN|DAT|INS)]			LemaMorf +[DEK] Kas Num
[ADI][SIN](.*?)[AMM][PART]>(.*?)[(PAR)]						LemaMorf +[DEK] Kas [MG]
