type=~'fmv'	type=~'subj'	=1	x2+x1
# Eg_SUBJ las_FMV aldri_ADV => I_SUBJ never_ADV read_FMV 
type=~'fmv'	type=~'adv'	=1	x2+x1

# Í dag fór ein bilur út_ADV [av vegnum]_ADV
