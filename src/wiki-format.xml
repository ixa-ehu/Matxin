<?xml version="1.0" encoding="ISO-8859-1"?>
<format name="txt">
  <options>
    <largeblocks size="8192"/>
    <input encoding="ISO-8859-1"/>
    <output encoding="ISO-8859-1"/>
    <tag-name regexp='[^ \n\t\r]'/>
    <escape-chars regexp='[][\\/@&lt;&gt;^]'/>
    <space-chars regexp='[ \n\t\r$*]'/>
    <case-sensitive value="no"/>
  </options>

  <rules>
    <format-rule type="close" eos="yes" name="quot" priority="1">
      <tag regexp='[&quot;&#x027;]\n'/>
    </format-rule>

    <format-rule type="close" eos="no" name="quot" priority="1">
      <tag regexp='[&quot;&#x027;]\r'/>
    </format-rule>

<!-- Headlines in wiki texts-->
    <format-rule type="open" eos="no" name="headline" priority="2">
      <tag regexp='^=+'/>
    </format-rule>

    <format-rule type="close" eos="yes" name="headline" priority="3">
      <tag regexp='=+[ \n\t\r]*(\r|\n|$)'/>
    </format-rule>


    <format-rule type="empty" eos="yes" priority="4">
      <tag regexp='(("\n")|("\r\n"))+'/>
    </format-rule>

    <format-rule type="open" eos="no" name="quot" priority="5">
      <tag regexp='^[&quot;&#x027;]'/>
    </format-rule>

    <format-rule type="open" eos="no" name="quot" priority="5">
      <tag regexp='[ \n\t\r][&quot;&#x027;]'/>
    </format-rule>

    <format-rule type="close" eos="no" name="quot" priority="6">
      <tag regexp='[&quot;&#x027;][ \n\t\r]'/>
    </format-rule>

    <format-rule type="close" eos="yes" name="quot" priority="6">
      <tag regexp='[&quot;&#x027;][.,:;]'/>
    </format-rule>


<!-- Imagenes without caption-->
    <format-rule type="empty" eos="no" priority="7">
      <tag regexp='\[\[(Fitxategi|Archivo|File):[^\]]+(\|(thumb|thumbnail|frame|right|left|center|none|[0-9]+px))*\]\]'/>
    </format-rule>


<!-- bold & italic in wiki texts-->
    <format-rule type="open" eos="no" name="italicbold" priority="8">
      <tag regexp='[ \n\t\r]&apos;&apos;&apos;&apos;&apos;'/>
    </format-rule>

    <format-rule type="open" eos="no" name="italicbold" priority="8">
      <tag regexp='^&apos;&apos;&apos;&apos;&apos;'/>
    </format-rule>

    <format-rule type="close" eos="no" name="italicbold" priority="8">
      <tag regexp='&apos;&apos;&apos;&apos;&apos;'/>
    </format-rule>

<!-- bold in wiki texts-->
    <format-rule type="open" eos="no" name="bold" priority="9">
      <tag regexp='[ \n\t\r]&apos;&apos;&apos;'/>
    </format-rule>

    <format-rule type="open" eos="no" name="bold" priority="9">
      <tag regexp='^&apos;&apos;&apos;'/>
    </format-rule>

    <format-rule type="close" eos="no" name="bold" priority="9">
      <tag regexp='&apos;&apos;&apos;'/>
    </format-rule>

<!-- italic in wiki texts-->
    <format-rule type="open" eos="no" name="italic" priority="10">
      <tag regexp='[\t ]&apos;&apos;'/>
    </format-rule>

    <format-rule type="open" eos="no" name="italic" priority="10">
      <tag regexp='^&apos;&apos;'/>
    </format-rule>

    <format-rule type="close" eos="no" name="italic" priority="10">
      <tag regexp='&apos;&apos;'/>
    </format-rule>




<!-- bulled and numerated lists in wiki texts-->
    <format-rule type="empty" eos="no" priority="11">
      <tag regexp='^[\*\#:;]+'/>
    </format-rule>

<!-- simple links in wiki texts-->
    <format-rule type="open" eos="no" name="link" inWord="yes" priority="12">
      <tag regexp='\[\[(Fitxategi|Archivo|File):[^\]]+(\|(thumb|thumbnail|frame|right|left|center|none|[0-9]+px))*\|'/>
    </format-rule>

    <format-rule type="open" eos="no" name="link" inWord="yes" priority="13">
      <tag regexp='\[\[[^\]]+\|'/>
    </format-rule>

    <format-rule type="open" eos="no" name="link" inWord="yes" priority="15">
      <tag regexp='\[\[\#?'/>
    </format-rule>

    <format-rule type="close" eos="no" name="link" inWord="yes" priority="15">
      <tag regexp='\]\]'/>
    </format-rule>

    <format-rule type="open" eos="no" name="ext-link" inWord="yes" priority="16">
      <tag regexp='\[http:[^ ]+ '/>
    </format-rule>

    <format-rule type="close" eos="no" name="ext-link" inWord="yes" priority="18">
      <tag regexp='\]'/>
    </format-rule>

    <format-rule type="open" eos="no" name="ref" priority="20">
      <tag regexp='"&lt;ref"(" "[^&gt;]*)?"&gt;"'/>
    </format-rule>

    <format-rule type="close" eos="no" name="ref" priority="22">
      <tag regexp='"&lt;/ref"(" "[^&gt;]*)?"&gt;"'/>
    </format-rule>

    <format-rule type="empty" eos="no" priority="25">
      <tag regexp='\{\{[^\}]+\}\}'/>
    </format-rule>

    <format-rule type="empty" eos="yes" priority="50">
      <tag regexp='[\[\]\(\)\{\}]'/>
    </format-rule>


  </rules>
</format>
